function integr_driver
    f = @(x) 11 * x.^10;
    X = linspace(-1, 1, 201);
    Y = f(X);
    plot(X, Y)
    simple_integration(Y, 0.01, 1)
    simple_integration(Y, 0.01, 2)
    simple_integration(Y, 0.01, 3)
    simple_integration(Y, 0.01, 4)

    
    for d=1:4
        for i=1:1000
            num_points = (d-1) * i + 1;
            X = linspace(-1, 1, num_points);
            Y = f(X);
            h = 4 / (num_points - 1);
        end
    end
end